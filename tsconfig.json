{
  "compilerOptions": {
    "noImplicitAny": true,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "module": "ESNext"
  }
}
